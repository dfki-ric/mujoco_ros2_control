<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro" name='USB_Male'>
  <mujoco>
    <compiler meshdir ="$(find panda_simulation)/meshes/visual"/>
  </mujoco>
  <link name="world"/>
  <link name="USB_Male_link">
    <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
    </inertial>
    <visual>
      <geometry>
        <mesh filename="file://$(find panda_simulation)/meshes/visual/USB_Male.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="file://$(find panda_simulation)/meshes/visual/USB_Male.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>
  <gazebo reference="USB_Male_link">
    <pose>0.4 0 1.0 0 0 0</pose>
  </gazebo>
  <joint name="fixed_joint" type="floating">
    <parent link="world"/>
    <child link="USB_Male_link"/>
    <origin rpy="0 0 0" xyz="0.3 0 1.8"/>
  </joint>

</robot>
